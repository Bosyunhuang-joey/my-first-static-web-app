{
  "routes": [
    {
      "route": "/authenticated/*",
      "allowedRoles": [
        "contributor"
      ]
    },
      {
      "route": "/anonymous/*",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/*.{png,jpg,webp,css,js,xml,ico,html,svg,webmanifest}"
    ]
  }
}
